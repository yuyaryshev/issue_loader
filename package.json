    "name": "issue_loader",
    "version": "1.0.0",
    "private": true,
    "description": "Load issues from Jira and post them to Oracle DB",
    "main": "index.js",
    "scripts": {
        "test": "jest --rootDir ts_out",
        "coverage": "jest --rootDir ts_out --coverage --coverageReporters=lcov  --coverageReporters=text-summary",
        "coverage-mocha": "nyc --reporter=lcov --reporter=text-summary npm run test",
        "test-mocha": "mocha --config=.mocharc.ut.js",
        "test-jenkins": "JUNIT_REPORT_PATH=/report.xml mocha --colors --reporter mocha-jenkins-reporter",
        "cov2": "mocha --config=.mocharc.cov.js",
        "nyc": "npm run coverage",
        "start": "node --trace-warnings ts_out/src/main.js run",
        "deploy": "node --trace-warnings ts_out/src/main.js deploy",
        "watch": "babel src --config-file ./babel.ts.config.js --out-dir ts_out/src --extensions \".ts,.tsx\" --source-maps inline -w",
        "web_server": "webpack --config webpack.react.config.js --watch",
        "build": "babel src --config-file ./babel.ts.config.js --out-dir ts_out/src --extensions \".ts,.tsx\" --source-maps inline && webpack --config webpack.react.config.js",
        "build_cov": "babel src --config-file ./babel.cov.config.js --out-dir cov_out/src --extensions \".ts,.tsx\" --source-maps inline && webpack --config webpack.react.config.js",
        "build_cov2": "babel src --config-file ./babel.cov.config.js --out-dir cov_out/src --extensions \".ts,.tsx\" --source-maps inline",
        "build_run": "babel src --config-file ./babel.ts.config.js --out-dir ts_out/src --extensions \".ts,.tsx\" --source-maps inline && webpack --config webpack.react.config.js && node ts_out/src/main.js run",
        "web_server_dev": "webpack-dev-server --config webpack.react.config.js --open",
        "ycplmon": "node ..\\ycplmon_amaglion\\ts_out\\src\\ycplmon.js src",
        "tsc": "tsc"
    },
